    pipeline {
        agent {
            docker {
                image 'maven:3.9.6-openjdk-21'  // docker for Java and maven
            }
        }
        stages {
            stage('Pre-requisites') {
                steps {
                    sh 'java -version' // Verify Java version
                    sh 'mvn -version' // Example build command
                }
            }
            stage('Test') {
                steps {
                    sh 'mvn clean install' // Example test command
                }
            }
            
             stage('Build') {
                steps {
                    sh 'mvn clean Build' // Example test command
                }
            }
            
        }
    }
